extends head

block content
  h1#bigTitle(class="text-center")= title
  
  div#mainDisplay(class="col-lg-12 col-md-12 col-sm-12 col-xs-12")
    h3#highLevel(class="text-center") High Level Overview
    
    div#vgtarget(class="col-lg-12 col-md-12 col-sm-12 col-xs-12")
  
  div#particularDisplays(class="col-lg-offset-4 col-lg-4 col-md-offset-4 col-md-4")
    h3#lowLevel(class="text-center") Specifics
    
    if message
      p(class="error text-center no-margin") #{message}
    
    form#countyNameQuery(action="/county", class="text-center", method="post")
      label Enter county name to get details about that county 
      
      input(name="countyname", type="text", class="form-control", placeholder="County", required="true")
      
      button(type="submit", class="form-control btn-success") Get Details
    
  include index.foot.jade
    
  
  
